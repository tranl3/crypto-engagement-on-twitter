    }
   ],
   "source": [
    "engagement_metrics_table = pd.DataFrame(brands_followers.groupby('brand')['follower_id'].count().sort_values())\n",
    "engagement_metrics_table['Average Retweet Count'] = brands_tweets.groupby(\"screen_name\").retweet_count.mean()\n",
    "engagement_metrics_table['Average Like Count'] = brands_tweets.groupby(\"screen_name\").like_count.mean()\n",
    "engagement_metrics_table['Average Number Retweets per Brand’s follower'] = engagement_metrics_table['Average Retweet Count'] / engagement_metrics_table['follower_id'] \n",
    "engagement_metrics_table['Average Number Likes per Brand’s follower'] = engagement_metrics_table['Average Like Count'] / engagement_metrics_table['follower_id'] \n",
    "engagement_metrics_table"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "................................................................................................................................................................................................................................................"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Type of tweets: Retweets & Total Tweets"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Original Tweets"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 115,
   "metadata": {},
   "outputs": [],
   "source": [
    "condition_to_select_row = (brands_tweets[\"is_retweet\"] == 0)\n",
    "brands_tweets_original = brands_tweets.loc[condition_to_select_row]"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Retweets"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 116,
   "metadata": {},
   "outputs": [],
   "source": [
    "condition_to_select_row = (brands_tweets[\"is_retweet\"] != 0)\n",
    "brands_retweets = brands_tweets.loc[condition_to_select_row]"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Summary"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 936x396 with 2 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "fig = plt.figure(figsize=(13, 5.5)) #specify figure size\n",
    "\n",
    "#Absolute distribution\n",
    "plt.subplot(1, 2, 1)\n",
    "ax1 = brands_tweets.groupby(['screen_name', 'is_retweet'])['is_retweet'].count().unstack().plot.bar(rot=0, ax=plt.gca(), width=0.7, \n",
    "                                                                                                    color=['pink', 'lightsalmon'])\n",
    "plt.title('Retweets vs. Original Tweets by Brand:\\n Absolute Distribution') \n",
    "#plot bar labels\n",
    "for p in ax1.patches:\n",
    "    ax1.annotate(str(p.get_height()), (p.get_x() * 1.02, p.get_height() * 0.5))\n",
    "\n",
    "L = plt.legend(bbox_to_anchor=(1,1))\n",
    "L.get_texts()[0].set_text('Original Tweets')\n",
    "L.get_texts()[1].set_text('Retweets')\n",
    "\n",
    "plt.xlabel('Brand', fontsize=10)\n",
    "plt.ylabel('Count', fontsize=10)\n",
    "    \n",
    "#Relative distribution\n",
    "plt.subplot(1, 2, 2)\n",
    "ax2 = brands_tweets.groupby(['screen_name', 'is_retweet'])['is_retweet'].size().groupby(level=0).apply(\n",
    "    lambda x: 100 * x / x.sum()).unstack().plot(kind='bar',stacked=True, rot=0, ax=plt.gca(), color=['pink', 'lightsalmon'])\n",
    "plt.gca().yaxis.set_major_formatter(mtick.PercentFormatter())\n",
    "plt.title('Retweets vs. Original Tweets by Brand:\\n Relative Distribution') \n",
    "#plot bar labels\n",
    "for p, q in zip(ax2.patches[0:4], ax2.patches[4:8]):\n",
    "    ax2.annotate(str(round(p.get_height(),2)) + '%', (p.get_x()*1.025, p.get_height()/2))\n",
    "    ax2.annotate(str(round(q.get_height(),2)) + '%', (q.get_x()*1.025, q.get_height()/2+p.get_height()))\n",
    "\n",
    "L = plt.legend(bbox_to_anchor=(0.3,0.15))\n",
    "L.get_texts()[0].set_text('Original Tweets')\n",
    "L.get_texts()[1].set_text('Retweets')\n",
    "\n",
    "plt.xlabel('Brand', fontsize=10)\n",
    "plt.ylabel('Percentage', fontsize=10)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "................................................................................................................................................................................................................................................"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Time of Posting - Original Tweets ONLY"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "def date_string_to_month(date_string):\n",
    "    date_obj = datetime.datetime.strptime(date_string, '%Y-%m-%d %H:%M:%S')\n",
    "    month = date_obj.month\n",
    "    return month\n",
    "\n",
    "def date_string_to_day(date_string):\n",
    "    date_obj = datetime.datetime.strptime(date_string, '%Y-%m-%d %H:%M:%S')\n",
    "    day = date_obj.day\n",
    "    return day\n",
    "\n",
    "def date_string_to_isoweekday(date_string):\n",
    "    date_obj = datetime.datetime.strptime(date_string, '%Y-%m-%d %H:%M:%S')\n",
    "    isoweekday = date_obj.isoweekday()\n",
    "    return isoweekday\n",
    "\n",
    "def date_string_to_hour(date_string):\n",
    "    date_obj = datetime.datetime.strptime(date_string, '%Y-%m-%d %H:%M:%S')\n",
    "    hour = date_obj.hour\n",
    "    return hour"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
