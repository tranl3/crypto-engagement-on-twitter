    "fig = plt.figure(figsize=(5, 5)) #specify figure size\n",
    "types = ['RobinhoodApp', 'binance', 'coinbase', 'eToro']\n",
    "y_coords = brands_tweets.groupby('screen_name').retweet_count.mean()\n",
    "x_coords = brands_tweets.groupby('screen_name').like_count.mean()\n",
    "\n",
    "for i,type in enumerate(types):\n",
    "    x = x_coords[i]\n",
    "    y = y_coords[i]\n",
    "    plt.scatter(x, y, marker='x', color='blue')\n",
    "    plt.text(x+0.3, y+0.3, type, fontsize=9)\n",
    "\n",
    "plt.ylabel('Average Retweet Count', fontsize=10)\n",
    "plt.xlabel('Average Like Count', fontsize=10)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
