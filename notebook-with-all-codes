      ]
     },
     "execution_count": 56,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "sps_list = []\n",
    "\n",
    "# Iterate through each car brands and calculate the Social Perception Scores\n",
    "for brand in brands_followers.brand.unique():\n",
    "    brand_follower_ids = set(brands_followers.loc[brands_followers.brand==brand].follower_id)\n",
    "    \n",
    "    sps_finance = social_perception_score(brand_follower_ids, exemplar_finance_followers)\n",
    "    sps_luxury = social_perception_score(brand_follower_ids, exemplar_luxury_followers)\n",
    "    sps_environmental = social_perception_score(brand_follower_ids, exemplar_environmental_followers)\n",
    "    \n",
    "    # Store the calculated Social Perception Scores in a list of list\n",
    "    new_row = [brand, sps_finance, sps_luxury, sps_environmental]\n",
    "    sps_list.append(new_row)\n",
    "\n",
    "# Turn the list of rows into a pandas dataframe\n",
    "sps_df = pd.DataFrame(sps_list,\n",
    "                     columns =['Brand', 'SPS_finance', 'SPS_luxury', \"SPS_environment\"])\n",
    "\n",
    "sps_df"
   ]
  },
