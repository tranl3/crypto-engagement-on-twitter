   ],
   "source": [
    "#Check for duplicate tweets\n",
    "if (brands_tweets.shape[0] - brands_tweets.duplicated(keep='first').sum()) == brands_tweets.shape[0]:\n",
    "    print (\"There is no duplicate tweets.\")\n",
    "else:\n",
    "    print(\"There is duplicate tweets.\")\n",
    "    duplicate_tweets = brands_tweets[brands_tweets.duplicated()]\n",
    "    print(duplicate_tweets)"
   ]
