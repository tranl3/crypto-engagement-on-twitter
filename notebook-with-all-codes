     "output_type": "execute_result"
    }
   ],
   "source": [
    "followers_df.groupby('follower_of_brand').friends_count.median()\n",
    "#we accidentally re-run the codes (pulling brand followers' friends & followers) after writing the report \n",
    "#so the numbers shown here and the numbers in the report will be slightly different"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 76,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "follower_of_brand\n",
       "RobinhoodApp    57\n",
       "binance         20\n",
       "coinbase        22\n",
       "eToro           72\n",
       "Name: followers_count, dtype: int64"
      ]
     },
     "execution_count": 76,
     "metadata": {},
     "output_type": "execute_result"
