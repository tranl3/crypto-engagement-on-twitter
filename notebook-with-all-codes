      "<ipython-input-46-0729b6d39578>:16: SettingWithCopyWarning: \n",
      "A value is trying to be set on a copy of a slice from a DataFrame.\n",
      "Try using .loc[row_indexer,col_indexer] = value instead\n",
      "\n",
      "See the caveats in the documentation: https://pandas.pydata.org/pandas-docs/stable/user_guide/indexing.html#returning-a-view-versus-a-copy\n",
      "  brands_tweets_original['mention_or_not'] = brands_tweets_original['user_mentioned'].apply(mention_or_not)\n"
     ]
    }
   ],
   "source": [
    "def mention_count(sentence):\n",
    "    num_user_mention = sentence.split(\",\")\n",
    "    if num_user_mention == ['']:\n",
    "        return 0\n",
    "    else:\n",
    "        return len(num_user_mention)\n",
    "\n",
    "brands_tweets_original['num_user_mention'] = brands_tweets_original['user_mentioned'].apply(mention_count)\n",
    "\n",
    "def mention_or_not(sentence):\n",
    "    if mention_count(sentence) >= 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "brands_tweets_original['mention_or_not'] = brands_tweets_original['user_mentioned'].apply(mention_or_not)"
   ]
  },
  {
   "cell_type": "code",
