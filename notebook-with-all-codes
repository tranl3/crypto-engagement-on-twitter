     "execution_count": 89,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# using spacy library for preprocessing\n",
    "nlp = spacy.load('en_core_web_sm')\n",
    "\n",
    "# defining the brands names we want to exclude\n",
    "brands=[\"binance\", \"bnb\", \"coinbase\", \"eToro\", \"etoro\", \"robinhoodApp\", \"robinhood\"]\n",
    "\n",
    "# defining a preprocessing function\n",
    "def my_tokenizer(doc):\n",
    "    return [token.lemma_ for token in nlp(doc.lower()) # do lemmatization\n",
    "            if token.pos_ in {'NOUN', 'VERB', 'PROPN', 'ADJ', 'ADV'}\n",
    "            and not token.is_stop    \n",
    "            and len(token.text) > 1  \n",
    "            and token.text not in brands \n",
    "            and token.text[0] != '@'\n",
    "            and token.text[0] != '#'\n",
    "           ]\n",
    "\n",
    "# setting the thresholds\n",
    "tfidf_vectorizer = TfidfVectorizer(analyzer='word', \n",
    "                                   ngram_range=(1,2),\n",
    "                                   min_df=70, \n",
    "                                   max_df=0.3,\n",
    "                                   tokenizer = my_tokenizer)\n",
    "\n",
    "X_tfidf = tfidf_vectorizer.fit_transform(documents)\n",
    "\n",
    "vocabs = tfidf_vectorizer.get_feature_names()\n",
    "\n",
    "vocabs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 91,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "11018"
      ]
     },
     "execution_count": 91,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "documents_filtered = [[vocabs[index] for index in X_tfidf.tolil().rows[r]]\n",
    "                       for r in range(0, X_tfidf.shape[0])]\n",
    "\n",
    "kept_indices = [i for i, doc in enumerate(documents_filtered) if len(doc) > 0]\n",
    "\n",
    "len (kept_indices)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "##### Step 2: Fitting Supervised LDA"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 92,
   "metadata": {},
   "outputs": [],
   "source": [
    "corpus = tp.utils.Corpus()\n",
    "\n",
    "for index in kept_indices:\n",
    "    corpus.add_doc(documents_filtered[index])    "
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "###### Choosing the number of topics"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 93,
   "metadata": {},
   "outputs": [],
   "source": [
    "train_idx, test_idx = train_test_split(kept_indices, train_size=0.7, test_size=0.3, random_state=123)\n",
    "corpus_train = tp.utils.Corpus()\n",
    "\n",
    "for index in train_idx:\n",
    "    corpus_train.add_doc(documents_filtered[index])    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 94,
   "metadata": {},
   "outputs": [],
   "source": [
    "words_test = []\n",
    "for index in test_idx:\n",
    "    words_test.extend(documents_filtered[index])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 95,
   "metadata": {},
   "outputs": [],
   "source": [
    "evaluation_results = []\n",
    "for ntopics in range(3, 20): # try 3 to 20 topics\n",
    "    mdl_ntopic = tp.LDAModel(corpus=corpus, k=ntopics, tw=tp.TermWeight.IDF, seed=543)\n",
    "    mdl_ntopic.train(10000)\n",
    "    coh_umass = tp.coherence.Coherence(mdl_ntopic, coherence='u_mass').get_score()\n",
    "    coh_cv = tp.coherence.Coherence(mdl_ntopic, coherence='c_v').get_score()\n",
    "    evaluation_results.append((ntopics, coh_umass, coh_cv))"
   ]
  },
  {
