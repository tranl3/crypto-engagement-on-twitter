   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Done with pulling followers of brand binance\n",
      "Done with pulling followers of brand coinbase\n",
      "Done with pulling followers of brand eToro\n",
      "Done with pulling followers of brand RobinhoodApp\n"
     ]
    }
   ],
   "source": [
    "con=sqlite3.connect(\"brands_followers.db\")\n",
    "cur=con.cursor()\n",
    "\n",
    "cur.execute(\"CREATE TABLE IF NOT EXISTS brands_followers(brand,follower_id)\")\n",
    "con.commit()\n",
    "\n",
    "brands=[\"binance\", \"coinbase\", \"eToro\", \"RobinhoodApp\"]\n",
    "\n",
    "for brand in brands:\n",
    "    for follower_id in tweepy.Cursor(api.followers_ids, screen_name=brand).items():\n",
    "        cur.execute(\"INSERT INTO brands_followers VALUES(?,?)\", \n",
    "               (brand, follower_id))        \n",
    "        con.commit()\n",
    "    print(\"Done with pulling followers of brand {}\".format(brand))"
   ]
  },
