  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Done with pulling followers of nytimesbusiness\n",
      "Done with pulling followers of WSJmarkets\n",
      "Done with pulling followers of bespokeinvest\n",
      "Done with pulling followers of Stocktwits\n",
      "Done with pulling followers of Benzinga\n"
     ]
    }
   ],
   "source": [
    "con=sqlite3.connect(\"exemplar_finance_followers.db\")\n",
    "cur=con.cursor()\n",
    "\n",
    "cur.execute(\"CREATE TABLE IF NOT EXISTS exemplar_finance_followers(exemplar,follower_id)\")\n",
    "con.commit()\n",
    "\n",
    "exemplars_finance = [\"nytimesbusiness\", \"WSJmarkets\", \"bespokeinvest\", \"Stocktwits\", \"Benzinga\"]\n",
    "\n",
    "# We will pull all followers for exemplars\n",
    "\n",
    "for handle in exemplars_finance:\n",
    "    for follower_id in tweepy.Cursor(api.followers_ids, screen_name=handle).items():\n",
    "        cur.execute(\"INSERT INTO exemplar_finance_followers VALUES(?,?)\", \n",
    "               (handle, follower_id))        \n",
    "        con.commit()\n",
    "    print(\"Done with pulling followers of {}\".format(handle))"
   ]
  },
