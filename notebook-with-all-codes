   ],
   "source": [
    "topic_term_dists = np.stack([slda_model.get_topic_word_dist(k) for k in range(slda_model.k)])\n",
    "doc_topic_dists = np.stack([doc.get_topic_dist() for doc in slda_model.docs])\n",
    "doc_topic_dists /= doc_topic_dists.sum(axis=1, keepdims=True)\n",
    "\n",
    "topic_dists_df = pd.DataFrame(data=doc_topic_dists, columns=topic_labels)\n",
    "topic_dists_df['screen_name'] = [brands_tweets.screen_name[i] for i in kept_indices]\n",
    "\n",
    "topic_dists_df.tail()"
   ]
