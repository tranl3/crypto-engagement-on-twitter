  {
   "cell_type": "code",
   "execution_count": 66,
   "metadata": {},
   "outputs": [],
   "source": [
    "def list_chunking(input_list, num_elem_per_chunk):\n",
    "    def _chunking_iterator():\n",
    "        for i in range(0, len(input_list), num_elem_per_chunk):\n",
    "            yield input_list[i:(i+num_elem_per_chunk)]    \n",
    "    return list(_chunking_iterator())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 67,
   "metadata": {},
   "outputs": [],
   "source": [
    "def brand_follower_ids_to_user_objects(list_of_ids):\n",
    "    list_of_user_objects = []    \n",
    "    # api.lookup_users takes up to 100 IDs at a time\n",
    "    for sublist_of_ids in list_chunking(list_of_ids, 100):\n",
    "        sublist_of_user_objects = api.lookup_users(user_ids=sublist_of_ids)\n",
    "        list_of_user_objects.extend(sublist_of_user_objects)    \n",
    "    return list_of_user_objects"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 73,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Done with pulling user info for binance\n",
      "Done with pulling user info for coinbase\n",
      "Done with pulling user info for eToro\n",
      "Done with pulling user info for RobinhoodApp\n"
     ]
    }
   ],
   "source": [
    "num_sample_ids = 200\n",
    "\n",
    "brand_followers_dict = dict()\n",
    "\n",
    "# Iterate through each brand in the brand dataframe\n",
    "for brand in brands_followers.brand.unique():\n",
    "    # Find the list of followers of that brand\n",
    "    brand_follower_ids = list(brands_followers.loc[brands_followers.brand==brand].follower_id)\n",
    "    \n",
    "    # Convert the follower IDs from int to string\n",
    "    # because the Twitter API accept strings, not int\n",
    "    list_of_ids = list(map(str,brand_follower_ids))\n",
    "    \n",
    "    # Draw random sample of the follower IDs for faster analysis\n",
    "    # If you want to pull info for all followers, set sample_ids = list_of_ids\n",
    "    sample_ids = random.sample(list_of_ids, num_sample_ids)\n",
    "    \n",
    "    # Call our defined function to retrieve user objects for these IDs\n",
    "    list_of_user_objects = brand_follower_ids_to_user_objects(sample_ids)\n",
    "    \n",
    "    # Store these user objects in a dict\n",
    "    brand_followers_dict[brand] = list_of_user_objects\n",
    "    \n",
    "    print('Done with pulling user info for {}'.format(brand))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 74,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Extract the following counts and follower counts into a dataframe\n",
    "\n",
    "followers_info = []\n",
    "\n",
    "for brand, user_objects in brand_followers_dict.items():\n",
    "    for user in user_objects:\n",
    "        info = [ brand , user.friends_count , user.followers_count ]\n",
    "        followers_info.append( info )\n",
    "\n",
    "followers_df = pd.DataFrame(followers_info, \n",
    "                            columns =['follower_of_brand', 'friends_count', 'followers_count'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 75,
   "metadata": {},
